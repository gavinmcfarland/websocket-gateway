{
    "name": "websocket-gateway",
    "version": "1.0.0",
    "description": "WebSocket Gateway",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "build": "tsup",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "start": "ts-node --esm src/index.ts"
    },
    "dependencies": {
        "chalk": "^5.4.1",
        "reconnecting-websocket": "^4.4.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "svelte": "^5.25.3",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "@types/ws": "^8.18.0",
        "@vitest/ui": "^3.0.9",
        "typescript": "^5.0.0",
        "vitest": "^3.0.9",
        "tsup": "^8.0.2"
    },
    "type": "module"
}
